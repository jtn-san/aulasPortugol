programa {
  funcao inicio() {
    real  valorHora, horasTrabalhadas, salarioMensal
 escreva("Digite o valor que você ganha por hora: ")
   leia(valorHora)

   escreva("Digite o número de horas trabalhadas no mês: ")
   leia(horasTrabalhadas)

   salarioMensal = valorHora * horasTrabalhadas

   escreva("O salário total no mês é: R$ ", salarioMensal)
   
  }
}
